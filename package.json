{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "test": "vitest --passWithNoTests",
    "lint": "run-p --print-label 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:biome": "biome check --error-on-warnings .",
    "fix": "run-s --print-label 'fix:*'",
    "fix:prettier": "prettier --write .",
    "fix:biome": "biome check --apply ."
  },
  "dependencies": {
    "@astrojs/react": "3.1.0",
    "astro": "4.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.5.10",
    "@biomejs/biome": "1.6.4",
    "@types/node": "20.10.6",
    "@types/react": "18.2.78",
    "@types/react-dom": "18.2.25",
    "globals": "15.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "prettier-plugin-astro": "0.12.3",
    "typescript": "5.3.3",
    "vitest": "1.5.0"
  }
}
