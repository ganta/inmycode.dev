{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "test:unit": "vitest --passWithNoTests",
    "test:e2e": "playwright test",
    "lint": "run-p --print-label 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:biome": "biome check --error-on-warnings .",
    "fix": "run-s --print-label 'fix:*'",
    "fix:prettier": "prettier --write .",
    "fix:biome": "biome check --apply .",
    "playwright": "playwright",
    "playwright:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@astrojs/react": "4.2.0",
    "astro": "5.3.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@biomejs/biome": "1.9.4",
    "@playwright/test": "1.50.1",
    "@types/node": "22.13.2",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "globals": "15.15.0",
    "netlify-cli": "18.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  }
}
