{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "test:unit": "vitest --passWithNoTests",
    "test:e2e": "playwright test",
    "lint": "run-p --print-label 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:biome": "biome check --error-on-warnings .",
    "fix": "run-s --print-label 'fix:*'",
    "fix:prettier": "prettier --write .",
    "fix:biome": "biome check --apply .",
    "playwright": "playwright",
    "playwright:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@astrojs/react": "3.6.2",
    "astro": "4.16.6",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "0.7.0",
    "@biomejs/biome": "1.8.1",
    "@playwright/test": "1.48.1",
    "@types/node": "22.5.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "globals": "15.11.0",
    "netlify-cli": "17.37.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.4.5",
    "vitest": "2.1.3"
  }
}
