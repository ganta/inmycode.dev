{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "test:unit": "vitest --passWithNoTests",
    "test:e2e": "playwright test",
    "lint": "run-p --print-label 'lint:*'",
    "lint:prettier": "prettier --check .",
    "lint:biome": "biome check --error-on-warnings .",
    "fix": "run-s --print-label 'fix:*'",
    "fix:prettier": "prettier --write .",
    "fix:biome": "biome check --apply .",
    "playwright": "playwright",
    "playwright:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@astrojs/react": "3.5.0",
    "astro": "4.10.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "0.7.0",
    "@biomejs/biome": "1.8.1",
    "@playwright/test": "1.44.1",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "globals": "15.4.0",
    "netlify-cli": "17.33.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.2",
    "prettier-plugin-astro": "0.14.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  }
}
